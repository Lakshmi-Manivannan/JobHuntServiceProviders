<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lakshmimanivannan.jobhuntappserviceproviders">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JobHuntAppServiceProviders">
        <activity android:name=".AcceptedRequests"></activity>
        <activity android:name=".AcceptRequest" />
        <activity
            android:name=".ServiceView"
            android:label="@string/title_activity_service_view"
            android:theme="@style/Theme.JobHuntAppServiceProviders.NoActionBar" />
        <activity android:name=".Profile" />
        <activity
            android:name=".ServiceProvidersView"
            android:label="@string/title_activity_service_providers_view" />
        <activity android:name=".SignUp2" />
        <activity android:name=".SignUp" />
        <activity android:name=".Login" />
        <activity android:name=".Splash_Screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>